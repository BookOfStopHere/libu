include common.mk
include ../../Makefile.conf

HTMLDOCDIR = ${DOCDIR}/libu/html/

all clean depend cleandepend:

install:
	@if [ -f index.html ]; then \
	    ${MKINSTALLDIRS} ${HTMLDOCDIR} && cp -r *.html *.css ${HTMLDOCDIR} \
    fi

uninstall:
	rm -rf ${HTMLDOCDIR}
